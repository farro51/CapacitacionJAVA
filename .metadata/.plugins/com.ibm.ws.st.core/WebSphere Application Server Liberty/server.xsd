                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="User"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Correlacionar una identidad de SAML con un usuario definido en el registro</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="Group"> 
