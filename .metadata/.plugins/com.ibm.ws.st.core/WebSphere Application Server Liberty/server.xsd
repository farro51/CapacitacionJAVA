    <xsd:complexType name="com.ibm.ws.webcontainer.cors"> 
        <xsd:annotation> 
            <xsd:documentation>Hace referencia a un procedimiento de seguridad de dominios cruzados para clientes basado en Javascript. </xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Uso compartido de recursos entre orígenes</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="domain" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>El nombre de dominio representa el URL que se configura con estos valores CORS. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nombre de dominio</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowedOrigins" use="optional" default="null" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Una lista separada por comas de los orígenes a los que se permite acceder al dominio configurado. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Orígenes permitidos</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowedMethods" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Una lista separada por comas de métodos HTTP que el dominio de origen puede utilizar cuando realiza peticiones al dominio configurado. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Métodos permitidos</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowedHeaders" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Una lista separada por comas de cabeceras HTTP cuyo uso está permitido por parte del dominio de origen cuando se realizan peticiones al dominio configurado. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Cabeceras permitidas</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="exposeHeaders" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Una lista separada por comas de cabeceras HTTP cuya exposición es segura en la API de llamada. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Exponer cabeceras</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="maxAge" use="optional" type="longType"> 
            <xsd:annotation> 
                <xsd:documentation>Longitud que indica cuántos segundos una respuesta puede estar en memoria caché en el navegador antes del lanzamiento</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Antigüedad máxima</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="allowCredentials" use="optional" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Un booleano que indica si las credenciales de usuario puede estar incluidas en la petición. </xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Permitir credenciales</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
