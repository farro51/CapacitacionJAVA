                                    <xsd:documentation>Aleatorio</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="SIZE"> 
                                <xsd:annotation> 
                                    <xsd:documentation>Tamaño</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="highThreshold" use="optional" default="80"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica cuándo se inicia la política de desalojo.</xsd:documentation> 
