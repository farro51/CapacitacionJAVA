                    <xsd:appinfo> 
                        <ext:label>Audiencias de confianza</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="id" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>ID exclusivo.</xsd:documentation> 
