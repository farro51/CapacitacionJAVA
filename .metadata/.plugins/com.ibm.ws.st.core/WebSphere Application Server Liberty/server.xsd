    <xsd:complexType name="com.ibm.ws.rest.api.discovery"> 
        <xsd:annotation> 
            <xsd:documentation>Configuración de las funciones de la API de descubrimiento para documentar sus API REST.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Descubrimiento de la API</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation> 
        <xsd:sequence> 
            <xsd:element name="webModuleDoc" minOccurs="0" maxOccurs="unbounded" type="com.ibm.ws.rest.api.discovery.webModuleDoc-factory"> 
                <xsd:annotation> 
                    <xsd:documentation>Configuración para cada módulo web que proporciona documentación de la API que se va a exponer.</xsd:documentation> 
                    <xsd:appinfo> 
                        <ext:label>Documento de módulo web</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:sequence> 
        <xsd:attribute name="maxSubscriptions" use="optional" default="20"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica el número máximo de clientes de API REST simultáneos que están a la escucha de actualizaciones.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Máximo de suscripciones</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:int"> 
                            <xsd:minInclusive value="0"> 
                            </xsd:minInclusive> 
                            <xsd:maxInclusive value="100"> 
                            </xsd:maxInclusive> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
            </xsd:simpleType> 
        </xsd:attribute> 
        <xsd:attribute name="apiName" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>El nombre de la API agregada.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Nombre de API</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="publicURL" use="optional" default="/api" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Especifica la raíz de contexto para los puntos finales de la documentación de la API. El URL no necesita una barra inclinada (/).</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>URL público</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="swaggerDefinition" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>Una ubicación de archivo o un URL de un fragmento de Swagger JSON o YAML, o una raíz de contexto de la aplicación que se ha de tomar como la definición de Swagger agregada.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Definición de Swagger</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
    <xsd:complexType name="com.ibm.ws.rest.api.discovery.webModuleDoc"> 
        <xsd:annotation> 
            <xsd:documentation>Configuración para cada módulo web que proporciona documentación de la API que se va a exponer.</xsd:documentation> 
            <xsd:appinfo> 
                <ext:label>Documento de módulo web</ext:label> 
            </xsd:appinfo> 
        </xsd:annotation><!--Idiom for "empty element context" (whitespace accepted and ignored)--> 
        <xsd:sequence> 
            <xsd:sequence/> 
        </xsd:sequence> 
        <xsd:attribute name="contextRoot" use="required" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>La raíz de contexto del módulo web para el que está proporcionando la documentación.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Raíz de contexto</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="enabled" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Un valor booleano que controla el proceso de documentación para este módulo web.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Habilitado</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="docURL" use="optional" type="xsd:string"> 
            <xsd:annotation> 
                <xsd:documentation>El URL de la documentación de este módulo web.  El URL puede ser relativo a la raíz de contexto si empieza con una barra inclinada (/) o absoluto si empieza por http o https.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>URL del documento</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="public" use="optional" default="true" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Un valor booleano que controla si se muestra el módulo web en la API pública.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Público</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
    </xsd:complexType> 
