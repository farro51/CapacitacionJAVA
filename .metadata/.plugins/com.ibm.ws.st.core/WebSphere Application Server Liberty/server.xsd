                                    <xsd:documentation>CHAR</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                            <xsd:enumeration value="TOPIC"> 
                                <xsd:annotation> 
                                    <xsd:documentation>TOPIC</xsd:documentation> 
                                </xsd:annotation> 
                            </xsd:enumeration> 
                        </xsd:restriction> 
                    </xsd:simpleType> 
                </xsd:union> 
