                    <xsd:appinfo> 
                        <ext:label>Nombre de grupo</ext:label> 
                    </xsd:appinfo> 
                </xsd:annotation> 
            </xsd:element> 
        </xsd:choice> 
        <xsd:anyAttribute processContents="skip"> 
